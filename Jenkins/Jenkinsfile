pipeline {
  agent any

  stages {
    stage('JAVA node') {
      agent {
        label 'JAVA'
      }
    steps {
      sh 'echo hello'
    }
  }
    stage('NODEJS node') {
      agent {
        label 'NODEJS'
      }
      steps {
        sh 'echo hey'
      }
    }
  }
}